<%= render 'shared_content/title', title: "All Scenarios" %>

<% Expansion.all.each do |expansion| %>
  <h3><%= link_to expansion.name, expansion_path(expansion) %></h3>

  <div class='row'>
    <div class='well col-md-6'>
      <ul>
    	 <% expansion.scenarios.each do |scenario| %>
          <li><%= link_to scenario.name, scenario_path(scenario) %></li>
    	 <% end %>
      </ul>
    </div>
  </div>

<% end %>