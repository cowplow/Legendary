<%= render 'shared_content/title', title: "All Henchmen Groups" %>

<% Expansion.all.each do |expansion| %>
  <h3><%= link_to expansion.name, expansion_path(expansion) %></h3>

  <div class='row'>
    <div class='well col-md-6'>
      <ul>
    	 <% expansion.henchmen.each do |henchman| %>
          <li><%= link_to henchman.name, henchman_path(henchman) %></li>
    	 <% end %>
      </ul>
    </div>
  </div>

<% end %>